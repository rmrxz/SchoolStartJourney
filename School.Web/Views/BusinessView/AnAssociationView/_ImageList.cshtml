@model IEnumerable<School.ViewModels.Attachments.BusinessImageVM>

<ul>
    @{
        if (Model.Count() == 0)
        {
            <div style="text-align:center;padding-top: 30px;font-size:22px"><p>暂无图片</p></div>
        }
        else
        {
            foreach (var item in Model)
            {
                <li><img src="@item.UploadPath" alt=""></li>
            }
        }
    }
</ul>

