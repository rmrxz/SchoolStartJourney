@model School.ViewModels.ApplicationOrganization.MessageNotifications.MessageNotificationVM
<link href="~/css/managerCenter/information_activityDetailed.css" rel="stylesheet" />
<div>
    <div class="operationDescription">
        <h5 class="title_per title-in"><a href="#" id="detailed-messageNotification" data-acId="@Model.ID">消息信息</a> </h5>
    </div>
    <div class="ac-info-images clearfix">
        <div class="activity-info">
            <label for="">活动名称：</label>
            <span>@Model.Name</span>
        </div>
        <div class="activity-info">
            <label for="">创建人：</label>
            <span>@Model.User.MobileNumber</span>
        </div>
        <div class="activity-info">
            <label for="">上限人数：</label>
            <span>@Model.CreateDateTime</span>
        </div>
        <div class="activity-info">
            <label for="">创建人：</label>
            <span>@Model.CreatedUser.Name</span>
        </div>
        <div class="activity-info">
            <label for="">活动介绍：</label>
            <span>@Model.Description</span>
        </div>
    </div>
</div>
